<table border="1">
   <tr>
      <th>Name</th>
      <th>Birthday</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Deparment</th>
      <th>Image</th>
   </tr>
   {{#each students }}
      <tr>
         <td>{{ name }}</td>
         <td>{{ dateFormat birthday "DD/MM/YYYY" }}
         </td>
         <td>{{ gender }}</td>
         <td>{{ email }}</td>
         <td>{{ department }}</td>
         <td>
            <img src="{{ image }}" width="100" height="100">
         </td>
      </tr>
   {{/each}}
</table>


